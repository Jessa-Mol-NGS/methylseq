process {
    withName: BISMARK_DEDUPLICATE {
        ext.args = params.run_umi_deduplication ? '--barcode' : ''
        publishDir = [
            [
                path: { "${params.outdir}/${params.aligner}/deduplicated/logs" },
                mode: params.publish_dir_mode,
                pattern: "*.txt"
            ]
        ]
    }
}
